{
  "name": "newswap-web-react-next",
  "private": true,
  "description": "newswap.org website",
  "version": "0.7.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "dev:testnet": "dotenv -e .env.testnet next",
    "dev:production": "dotenv -e .env.production next",
    "build:dev": "dotenv -e .env next build",
    "build:testnet": "dotenv -e .env.testnet next build",
    "build:production": "dotenv -e .env.production next build",
    "build": "dotenv -e .env.testnet build",
    "start": "next start",
    "export:dev": "dotenv -e .env next build && next export -o html",
    "export:testnet": "dotenv -e .env.testnet next build && next export -o html",
    "export:production": "dotenv -e .env.production next build && next export -o html",
    "export": "dotenv -e .env.testnet next build && next export -o html",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "i18next": "^19.7.0",
    "i18next-browser-languagedetector": "^6.0.1",
    "next": "^9.5.3",
    "next-images": "^1.4.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-countup": "^4.3.3",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.2",
    "react-id-swiper": "^4.0.0",
    "react-modal-video": "^1.2.3",
    "react-scroll": "^1.8.0",
    "react-scrollable-anchor": "^0.6.1",
    "react-visibility-sensor": "^5.1.1",
    "swiper": "^6.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.4",
    "prettier": "2.1.2"
  }
}
